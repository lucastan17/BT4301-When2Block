{"ast":null,"code":"import { LMap, //LIcon,\nLTileLayer, LMarker\n/*LControlLayers,\r\nLTooltip,\r\nLPopup,\r\nLPolyline,\r\nLPolygon,\r\nLRectangle,*/\n} from \"@vue-leaflet/vue-leaflet\";\nimport \"leaflet/dist/leaflet.css\";\nimport axios from 'axios';\nexport default {\n  name: 'HeaderBar',\n  props: {\n    msg: String\n  },\n  components: {\n    LMap,\n    //LIcon,\n    LTileLayer,\n    LMarker //LControlLayers,\n    //LTooltip,\n    //LPopup,\n    //LPolyline,\n    //LPolygon,\n    //LRectangle,\n\n  },\n\n  data() {\n    return {\n      url: 'https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png',\n      zoom: 11,\n      center: [1.3521, 103.8198],\n      markerLatLng: [1.3521, 103.75],\n      info: []\n    };\n  },\n\n  mounted: function () {\n    axios.get('https://api.data.gov.sg/v1/environment/2-hour-weather-forecast').then(response => {\n      this.info = response.data;\n      console.log(this.info.area_metadata[0]);\n    }); //console.log(this.info)\n  }\n  /*methods:{\r\n    goto(x,y){\r\n    }\r\n  }*/\n\n};","map":{"version":3,"mappings":"AAwDA,SACEA,IADF,EAEE;AACAC,UAHF,EAIEC;AACA;;;;;;AALF,OAWO,0BAXP;AAYA,OAAO,0BAAP;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;EACXC,IAAI,EAAE,WADK;EAEXC,KAAK,EAAE;IACHC,GAAG,EAAEC;EADF,CAFI;EAKXC,UAAU,EAAE;IACZR,IADY;IAEZ;IACAC,UAHY;IAIZC,OAJY,CAKZ;IACA;IACA;IACA;IACA;IACA;;EAVY,CALD;;EAiBXO,IAAG,GAAK;IACJ,OAAO;MACHC,GAAG,EAAE,oDADF;MAEHC,IAAI,EAAE,EAFH;MAGHC,MAAM,EAAE,CAAC,MAAD,EAAS,QAAT,CAHL;MAIHC,YAAY,EAAE,CAAC,MAAD,EAAS,MAAT,CAJX;MAKHC,IAAI,EAAE;IALH,CAAP;EAOH,CAzBU;;EA2BXC,OAAO,EAAE,YAAU;IACfZ,KAAK,CAACa,GAAN,CAAU,gEAAV,EACMC,IADN,CACWC,QAAO,IAAI;MACd,KAAKJ,IAAL,GAAYI,QAAQ,CAACT,IAArB;MACAU,OAAO,CAACC,GAAR,CAAY,KAAKN,IAAL,CAAUO,aAAV,CAAwB,CAAxB,CAAZ;IACF,CAJN,EADe,CAMf;EACJ;EACF;;;;;AAnCa,CAAf","names":["LMap","LTileLayer","LMarker","axios","name","props","msg","String","components","data","url","zoom","center","markerLatLng","info","mounted","get","then","response","console","log","area_metadata"],"sourceRoot":"","sourc